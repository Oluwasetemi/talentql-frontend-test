{
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"serve": "vite preview",
		"lint:eslint": "eslint . --ext ts,tsx,js",
		"lint:prettier": "prettier --write \"**/*.{js,json,ts,tsx}\"",
		"lint:types": "tsc",
		"lint:css": "stylelint './src/**/*.(ts|tsx)'",
		"lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:types",
		"test": "yarn lint && yarn test:unit",
		"test:unit": "jest --env=jest-environment-jsdom-sixteen",
		"test:unit:debug": "cross-env NODE_ENV=test node --inspect-brk `which jest` --runInBand"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.6.0",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"react-select": "^4.3.1",
		"react-shapes": "^0.1.0",
		"styled-components": "^5.3.0"
	},
	"devDependencies": {
		"@testing-library/dom": "^8.0.0",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.1.9",
		"@types/jest": "^26.0.23",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.7",
		"@types/styled-components": "^5.1.10",
		"@typescript-eslint/eslint-plugin": "^4.28.0",
		"@typescript-eslint/parser": "^4.28.0",
		"@vitejs/plugin-react-refresh": "^1.3.1",
		"eslint": "^7.29.0",
		"eslint-plugin-cypress": "^2.11.3",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jest": "^24.3.6",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-prettier-config": "^1.0.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "23.x",
		"jest-environment-jsdom-sixteen": "^2.0.0",
		"jest-styled-components": "^7.0.4",
		"jest-watch-typeahead": "^0.6.4",
		"prettier": "^2.3.2",
		"stylelint": "^13.13.1",
		"stylelint-config-recommended": "^5.0.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-processor-styled-components": "^1.10.0",
		"ts-jest": "23.10.0",
		"typescript": "^3.9.10",
		"vite": "^2.3.8"
	},
	"prettier": {
		"singleQuote": true,
		"semi": true,
		"trailingComma": "all",
		"useTabs": true,
		"arrowParens": "always"
	},
	"name": "talentql-frontend-test",
	"main": "src/main",
	"keywords": [],
	"author": "Oluwasetemi Ojo <setemiojo@gmail.com> (https://oluwasetemi.dev/)",
	"license": "MIT",
	"description": "Build a grid app that renders items from a json file. The items would be filtered by shapes and colors. All items would be selected by default, and the grid title should reflect the active filters."
}
